class Problem6 {
    public int difference() {
        int sumSquare = 0;
        int simpleSum = 0;
        for (int i = 1; i <= 100; i++) {
            sumSquare += Math.pow(i, 2);
            simpleSum += i;
        }
        int squareSum = (int)Math.pow(simpleSum, 2);
        return squareSum - sumSquare;
    }
}

public class Main {
    public static void main(String[] args) {
        Problem6 exercise = new Problem6();
        System.out.println(exercise.difference());
    }
}
